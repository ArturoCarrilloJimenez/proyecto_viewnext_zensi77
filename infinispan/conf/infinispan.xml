infinispan:
  server:
    data:
      root: /opt/infinispan/server/data
    endpoints:
      endpoint:
        hotrod-connector:
          security:
            authentication: true
        rest-connector:
          security:
            authentication: true
security:
  security-realms:
    default:
      properties-realm:
        users-properties:
          path: ${CONFIG_PATH}/identities.yaml
          plain-text: true